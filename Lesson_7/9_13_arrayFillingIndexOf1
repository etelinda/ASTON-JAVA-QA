package Lesson_1
  import java.util.Arrays;
    public class Main {
      public static void main(String[] args) {
private int[][] array = new int[100][100];
  for(int i = 0; i < array.length; i++) {
    array[i][i] = 1;
      if (i != 0) {
        array[i-1][i] = 1;
}
}
System.out.println(Arrays.deepToString(array));
}
}
