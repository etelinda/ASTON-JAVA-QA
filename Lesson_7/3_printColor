package Lesson_1
public class Main {
public static void main(String[] args) {
    public static void printColor(int value) {
    if (value < 0) {
        System.out.println("Красный");
    }   else if (value >= 0 && value <= 100) 
     System.out.println("Желтый");
     } else{
        System.out.println ("Зеленый");
                }
                }
}
